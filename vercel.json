{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.js" },
    {
      "src": "/client-login",
      "methods": ["POST"],
      "dest": "/api/client-login"
    },
    {
      "src": "/register-cliend",
      "methods": ["POST"],
      "dest": "/api/register-client"
    },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
